{"ast":null,"code":"// import axios\nimport axios from \"axios\";\nexport default {\n  name: \"EditMunicipio\",\n\n  data() {\n    return {\n      municipioName: \"\",\n      municipioArea: \"\",\n      municipioPresupuesto: \"\"\n    };\n  },\n\n  created: function () {\n    this.getMunicipioById();\n  },\n  methods: {\n    async getMunicipioById() {\n      try {\n        const response = await axios.get(`http://localhost:5000/municipios/${this.$route.params.id}`);\n        this.municipioName = response.data.NOMBRE_MUN;\n        this.municipioArea = response.data.AREA;\n        this.municipioPresupuesto = response.data.PRESUPUESTO;\n      } catch (err) {\n        console.log(err);\n      }\n    },\n\n    async updateMunicipio() {\n      try {\n        await axios.put(`http://localhost:5000/municipios/${this.$route.params.id}`, {\n          NOMBRE_MUN: this.municipioName,\n          AREA: this.municipioArea,\n          PRESUPUESTO: this.municipioPresupuesto\n        });\n        this.municipioName = \"\";\n        this.municipioArea = \"\";\n        this.municipioPresupuesto = \"\";\n        this.$router.push(\"/\");\n      } catch (err) {\n        console.log(err);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4CA;AACA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,eADO;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLC,mBAAa,EAAE,EADV;AAELC,mBAAa,EAAE,EAFV;AAGLC,0BAAoB,EAAE;AAHjB,KAAP;AAKD,GARY;;AASbC,SAAO,EAAE,YAAY;AACnB,SAAKC,gBAAL;AACD,GAXY;AAYbC,SAAO,EAAE;AACP,UAAMD,gBAAN,GAAyB;AACvB,UAAI;AACF,cAAME,QAAO,GAAI,MAAMT,KAAK,CAACU,GAAN,CACpB,oCAAmC,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAAG,EADrC,CAAvB;AAGA,aAAKV,aAAL,GAAqBM,QAAQ,CAACP,IAAT,CAAcY,UAAnC;AACA,aAAKV,aAAL,GAAqBK,QAAQ,CAACP,IAAT,CAAca,IAAnC;AACA,aAAKV,oBAAL,GAA4BI,QAAQ,CAACP,IAAT,CAAcc,WAA1C;AACF,OAPA,CAOE,OAAOC,GAAP,EAAY;AACZC,eAAO,CAACC,GAAR,CAAYF,GAAZ;AACF;AACD,KAZM;;AAcP,UAAMG,eAAN,GAAwB;AACtB,UAAI;AACF,cAAMpB,KAAK,CAACqB,GAAN,CACH,oCAAmC,KAAKV,MAAL,CAAYC,MAAZ,CAAmBC,EAAG,EADtD,EAEJ;AACEC,oBAAU,EAAE,KAAKX,aAD