{"ast":null,"code":"// import axios\nimport axios from \"axios\";\nexport default {\n  name: \"EditMunicipio\",\n\n  data() {\n    return {\n      municipioName: \"\",\n      productPrice: \"\"\n    };\n  },\n\n  created: function () {\n    this.getProductById();\n  },\n  methods: {\n    // Get Product By Id\n    async getProductById() {\n      try {\n        const response = await axios.get(`http://localhost:5000/products/${this.$route.params.id}`);\n        this.productName = response.data.product_name;\n        this.productPrice = response.data.product_price;\n      } catch (err) {\n        console.log(err);\n      }\n    },\n\n    // Update product\n    async updateProduct() {\n      try {\n        await axios.put(`http://localhost:5000/products/${this.$route.params.id}`, {\n          product_name: this.productName,\n          product_price: this.productPrice\n        });\n        this.productName = \"\";\n        this.productPrice = \"\";\n        this.$router.push(\"/\");\n      } catch (err) {\n        console.log(err);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4CA;AACA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,eADO;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLC,mBAAa,EAAE,EADV;AAELC,kBAAY,EAAE;AAFT,KAAP;AAID,GAPY;;AAQbC,SAAO,EAAE,YAAY;AACnB,SAAKC,cAAL;AACD,GAVY;AAWbC,SAAO,EAAE;AACP;AACA,UAAMD,cAAN,GAAuB;AACrB,UAAI;AACF,cAAME,QAAO,GAAI,MAAMR,KAAK,CAACS,GAAN,CACpB,kCAAiC,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAAG,EADnC,CAAvB;AAGA,aAAKC,WAAL,GAAmBL,QAAQ,CAACN,IAAT,CAAcY,YAAjC;AACA,aAAKV,YAAL,GAAoBI,QAAQ,CAACN,IAAT,CAAca,aAAlC;AACF,OANA,CAME,OAAOC,GAAP,EAAY;AACZC,eAAO,CAACC,GAAR,CAAYF,GAAZ;AACF;AACD,KAZM;;AAcP;AACA,UAAMG,aAAN,GAAsB;AACpB,UAAI;AACF,cAAMnB,KAAK,CAACoB,GAAN,CACH,kCAAiC,KAAKV,MAAL,CAAYC,MAAZ,CAAmBC,EAAG,EADpD,EAEJ;AACEE,sBAAY,EAAE,KAAKD,WADrB;AAEEE,uBAAa,EAAE,KAAKX;AAFtB,SAFI,CAAN;AAOA,aAAKS,WAAL,GAAmB,EAAnB;AACA,aAAKT,YAAL,GAAoB,EAApB;AACA,aAAKiB,OAAL,CAAaC,IAAb,CAAkB,GAAlB;AACF,OAXA,CAWE,OAAON,GAAP,EAAY;AACZC,eAAO,CAACC,GAAR,CAAYF,GAAZ;AACF;AAC